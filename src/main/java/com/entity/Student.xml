<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="com.entity.Student">
        <table name="student" schema="mybatis" />
        <attributes>
            <id name="studetnId">
                <column name="studetnId"/>
            </id>
            <basic name="name">
                <column name="name"/>
            </basic>
            <basic name="sex">
                <column name="sex"/>
            </basic>
            <basic name="age">
                <column name="age"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>

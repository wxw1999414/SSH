<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="com.entity.Studentclass">
        <table name="studentclass" schema="mybatis" />
        <attributes>
            <id name="studentId">
                <column name="studentId"/>
            </id>
            <basic name="className">
                <column name="className"/>
            </basic>
            <basic name="classId">
                <column name="classId"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
